    <operations>
        <operation name="tagGerman">
            <description>tags articles</description>
            <command>/opt/anaconda3/bin/python /opt/anaconda-scripts/ner-tomar.py ger ${input}</command>
            <inputs>
                <input name="input" required="true">
                    <description>URL Reference</description>
                </input>
            </inputs>
            <outputs>
                <stdout>
                    <description>output</description>
                </stdout>
            </outputs>
        </operation>
        <operation name="tagHungarian">
            <description>tags articles</description>
            <command>/opt/anaconda3/bin/python /opt/anaconda-scripts/ner-tomar.py hun ${input}</command>
            <inputs>
                <input name="input" required="true">
                    <description>URL Reference</description>
                </input>
            </inputs>
            <outputs>
                <stdout>
                    <description>output</description>
                </stdout>
            </outputs>
        </operation>
    </operations>
</tool>